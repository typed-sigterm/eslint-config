{
  "name": "@typed-sigterm/eslint-config",
  "type": "module",
  "version": "1.3.0",
  "packageManager": "pnpm@9.14.2",
  "description": "Typed SIGTERM's ESLint config",
  "author": "Typed SIGTERM",
  "license": "MIT",
  "homepage": "https://eslint-config.by-ts.top",
  "repository": {
    "type": "git",
    "url": "https://github.com/typed-sigterm/eslint-config"
  },
  "keywords": [
    "eslint-config"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": "20"
  },
  "scripts": {
    "build": "tsup",
    "build:inspector": "eslint-config-inspector build",
    "lint": "eslint . --flag unstable_ts_config",
    "prepare": "husky"
  },
  "peerDependencies": {
    "@antfu/eslint-config": "^3.0.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.0.0",
    "@commitlint/cli": "^19.4.1",
    "@commitlint/config-conventional": "^19.4.1",
    "@commitlint/types": "^19.0.3",
    "@eslint-react/eslint-plugin": "^1.12.4",
    "@eslint/config-inspector": "^0.5.4",
    "@unocss/eslint-plugin": "^0.64.0",
    "eslint": "^9.11.1",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "eslint-plugin-solid": "^0.14.3",
    "eslint-plugin-svelte": "^2.43.0",
    "husky": "^9.1.4",
    "semver": "^7.6.3",
    "svelte": "^5.0.0",
    "tsup": "^8.2.0",
    "typescript": "^5.5.3"
  }
}
