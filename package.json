{
  "name": "@typed-sigterm/eslint-config",
  "type": "module",
  "version": "1.0.1",
  "packageManager": "pnpm@9.5.0",
  "description": "Typed SIGTERM's ESLint config",
  "author": "Typed SIGTERM",
  "license": "MIT",
  "homepage": "https://eslint-config.by-ts.top/",
  "repository": {
    "type": "git",
    "url": "https://github.com/typed-sigterm/eslint-config"
  },
  "keywords": [
    "eslint-config"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": "20"
  },
  "scripts": {
    "build": "tsup",
    "inspector:build": "eslint-config-inspector build",
    "lint": "eslint .",
    "prepare": "husky"
  },
  "peerDependencies": {
    "@antfu/eslint-config": "^2.23.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.23.1",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@commitlint/types": "^19.0.3",
    "@eslint-react/eslint-plugin": "^1.5.27",
    "@eslint/config-inspector": "^0.5.2",
    "@unocss/eslint-plugin": "^0.61.5",
    "eslint": "npm:eslint-ts-patch@8.57.0-0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.8",
    "eslint-plugin-solid": "^0.13.2",
    "eslint-plugin-svelte": "^2.43.0",
    "eslint-ts-patch": "8.57.0-0",
    "husky": "^9.1.1",
    "semver": "^7.6.3",
    "svelte": "^4.2.18",
    "tsup": "^8.2.0",
    "tsx": "^4.16.2",
    "typescript": "^5.5.3"
  }
}
